.date{
  width:100%;
  height: 108px;
  background-color: white;
  margin-bottom:10px;
}
.date-icon {
  width:10px;
  height:6px;
  margin-left:2px;
}
.content{
  background-color: #f3f4f9;
}
.month {
  text-align:center;
  font-size: 14px;
  height:48px;
  line-height: 48px;
  width:100%;
  color:#3d3d3d;
  display:block;
  font-weight: 600;
}
@media screen and (min-width:375px) {
  .calendar-width,.calendar {
    width:397px;
  }
  .calendar-container {
    margin-right: 31px;
  }
}
@media screen and (max-width:375px) {
    .calendar-width,.calendar {
    width:360px;
    
  }
  .calendar-container {
    margin-right: 25px;
  }
}
@media screen and (max-width:384px) {
    .calendar-width,.calendar {
    width:369px;
    
  }
  .calendar-container {
    margin-right: 26px;
  }
}
@media screen and (max-width:360px) {
    .calendar-width,.calendar {
    width:345px;
    
  }
  .calendar-container {
    margin-right: 22px;
  }
}
@media screen and (max-width:320px) {
  .calendar-width,.calendar {
    width:304px;
  }
  .calendar-container {
    margin-right: 15.5px;
  }
}
.calendar {
  margin:0 auto;
}
.calendar-container,.calendar-container-last {
  display: inline-block;
}
.calendar-week {
  display: block;
  width:30px;
  font-size:12px;
  height:20px;
  color:#cccccc;
  text-align: center;
}
.calendar-day,.calendar-day-selected {
  display: block;
  width:30px;
  height:30px;
  font-size:14px;
  text-align: center;
  line-height: 30px;
}
.calendar-day-selected {
  color:white;
  background-image:url(/pages/image/day-selected.png);
}
.court-container {
  padding:0px 10px 0px 10px;
}
.court-info,.court-sort {
  display: inline-block;
}
.court-sort {
  vertical-align: top;
}
.court-sort-content {
  background-color: #85b2f6;
  border-radius: 4px;
  width:38px;
  height:38px;
  color:white;
  text-align: center;
  line-height: 38px;
  font-size:14px;
}
.court-sort-line {
  padding-bottom:54px;
  width: 1px;
  margin:0 auto;
  background: url(/pages/image/sort-line1.png) center;
}
@media screen and (min-width:375px) {
  .court-info-scroll {
    height:464px;
  }
  .court-info {
    width:336px;
  }
  .court-info-line {
    width:318px;
  }
}
@media screen and (max-width:375px) {
  .court-info-scroll {
    height:397px;
  }
  .court-info {
    width:300px;
  }
  .court-info-line {
    width:280px;
  }
}
@media screen and (max-width:384px) and (min-width:376px) {
  .court-info-scroll {
    height:368px;
  }
  .court-info {
    width:309px;
  }
  .court-info-line {
    width:289px;
  }
}
@media screen and (max-width:360px) {
  .court-info-scroll {
    height:368px;
  }
  .court-info {
    width:285px;
  }
  .court-info-line {
    width:265px;
  }
}
@media screen and (max-width:320px) {
  @media screen and (min-height:481px) {
    .court-info-scroll {
      height:296px;
    }
  }
  @media screen and (max-height:480px) {
    .court-info-scroll {
      height:208px;
    }
  }
  .court-info {
    width:245px;
  }
  .court-info-line {
    width:225px;
  }
}
.court-info {
  background-color: white;
  height:82px;
  border-radius: 5px;
  margin-left:17px;
  font-size: 14px;
  margin-bottom:10px;
  
}
.court-info-name,.court-info-addr {
  height:34px;
  line-height: 34px;
}
.court-info-name {
  float:left;
  margin-left:10px;
}
.court-info-addr {
  float:right;
  margin-right:10px;
}
.court-info-iaddr {
  width:12px;
  height:34px;
  background: url(/pages/image/icon-addr.png) center no-repeat;
  display: inline-block;
  vertical-align: top;
  margin-right: 5px;
}
.court-info-line {
  height:1px;
  margin:0 auto;
  display: block;
}
.court-info-time {
  height:46px;
  line-height:46px;
}
.court-info-clock {
  width:18px;
  height:46px;
  background: url(/pages/image/icon-clock.png) center no-repeat;
  display: inline-block;
  vertical-align: top;
  margin-left: 10px;
}