.date{
  width:100%;
  height: 108px;
  background-color: white;
  margin-bottom:10px;
}
.date-icon {
  width:10px;
  height:6px;
  margin-left:2px;
}
.content{
  background-color: #f3f4f9;
}
.month {
  text-align:center;
  font-size: 14px;
  height:48px;
  line-height: 48px;
  width:100%;
  color:#5c6b71;
  display:block;
  font-weight: 600;
}
@media screen and (min-width:375px) {
  .calendar-width,.calendar {
    width:397px;
  }
  .calendar-container {
    margin-right: 24px;
  }
}
@media screen and (max-width:375px) {
    .calendar-width,.calendar {
    width:360px;
    
  }
  .calendar-container {
    margin-right: 18px;
  }
}
@media screen and (max-width:384px)  and (min-width:376px)  {
    .calendar-width,.calendar {
    width:369px;
    
  }
  .calendar-container {
    margin-right: 19.5px;
  }
}
@media screen and (max-width:360px) {
    .calendar-width,.calendar {
    width:345px;
    
  }
  .calendar-container {
    margin-right: 15.5px;
  }
}
@media screen and (max-width:320px) {
  .calendar-width,.calendar {
    width:304px;
  }
  .calendar-container {
    margin-right: 8.5px;
  }
}
.calendar {
  margin:0 auto;
}
.calendar-container,.calendar-container-last {
  display: inline-block;
}
.calendar-week {
  display: block;
  width:36px;
  font-size:12px;
  height:16px;
  color:#cccccc;
  text-align: center;
}
.calendar-day,.calendar-day-selected {
  display: block;
  width:36px;
  height:36px;
  font-size:14px;
  color:#5c6b71;
  text-align: center;
  line-height: 36px;
}
.calendar-day-selected {
  color:white;
  background-image:url(http://ogkcxvdnd.bkt.clouddn.com/images/day-selected.png?v=20161113);
}
.court-container {
  padding:0px 10px 0px 10px;
}
.court-info,.court-sort,.court-info-selected {
  display: inline-block;
}
.court-sort {
  vertical-align: top;
}
.court-sort-content,.court-sort-content-sel {
  background-color: #bdcbcf;
  border-radius: 4px;
  width:38px;
  height:38px;
  color:white;
  text-align: center;
  line-height: 38px;
  font-size:14px;
}
.court-sort-content-sel {
  background-color: #39cafe;
}
.court-sort-line {
  padding-bottom:132px;
  width: 1px;
  margin:0 auto;
  background: url(http://ogkcxvdnd.bkt.clouddn.com/images/sort-line1.png?v=20161113) center;
}
/*iphone6plus*/
@media screen and (max-height:740px) {
  .court-info-scroll {
    height:462px;
  }
  .court-info,.court-info-selected {
    width:336px;
  }
  .court-info-line {
    width:318px;
  }
}
